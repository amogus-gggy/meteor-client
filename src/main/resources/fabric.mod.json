{
  "schemaVersion": 1,
  "name": "VoidStrike Client",
  "id": "voidstrike-client",
  "version": "${version}",
  "description": "Based utility mod.",
  "authors": [
    "MineGame159",
    "squidoodly",
    "seasnail",
    "amogusgggy"
  ],
  "contact": {
    "homepage": "https://meteorclient.com",
    "issues": "https://github.com/MeteorDevelopment/meteor-client/issues",
    "sources": "https://github.com/MeteorDevelopment/meteor-client",
    "discord": "https://meteorclient.com/discord"
  },
  "license": "GPL-3.0",
  "icon": "assets/voidstrike-client/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "voidstrike_dev.voidstrike_client.MeteorClient"
    ],
    "modmenu": [
      "voidstrike_dev.voidstrike_client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "voidstrike-client.mixins.json",
    "voidstrike-client-baritone.mixins.json",
    "voidstrike-indigo.mixins.json",
    "voidstrike-client-sodium.mixins.json",
    "voidstrike-client-lithium.mixins.json",
    "voidstrike-client-viafabricplus.mixins.json"
  ],
  "accessWidener": "voidstrike-client.accesswidener",
  "custom": {
    "voidstrike-client:build_number": "${build_number}",
    "voidstrike-client:color": "220,20,60",
    "voidstrike-client:commit": "${commit}",
    "modmenu": {
      "links": {
        "modmenu.discord": "https://meteorclient.com/discord"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${minecraft_version}"],
    "fabricloader": ">=${loader_version}"
  },
  "breaks": {
    "optifabric": "*",
    "feather": "*",
    "origins": "*",
    "sodium": "<0.7.0",
    "morechathistory": "*"
  }
}
